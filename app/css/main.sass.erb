/*------ VIEW GENERIC AND RESET ------*/
@font-face
    font-family: "decohomefont"
    src: url('<%= asset_data_uri("jandles.ttf") %>') format("truetype")

.logo
    font-family: "decohomefont" !important //, Helvetica, Arial, sans-serif
    font-size: 47px
    color: white
.footer
    position: absolute
    bottom: 0px
    text-align: center
    width: 100%
*
    scroll: hidden

.ui-content
    padding: 0
    overflow-y: hidden
    overflow-x: none
.ui-page
    background:
        image: url('background.jpg')
        size: 100% 100%
        repeat: no-repeat
    max-height: 100%
    max-width: 100%
img
    max-height: 100%
    width: 100%

.b-page
    box-sizing: border-box
    height: 100% !important
.b-wrap
    //background-color: transparent !important
    height: auto !important
    max-height: 100%
.b-counter
    background-color: transparent !important
    font-weight: bold
    font-size: 12px
.b-arrow div
    width: 44px !important
    background-size: 100% auto !important
    background-position: center center
//.b-arrow-prev
//    left: -47%
.b-arrow-next div
    background-position: center left !important
.b-arrow-prev div
    background-position: center right !important
    left: auto !important
    right: 0
#mybook
    margin-top: 1% !important

.ui-header
    text-align: center
    background: transparent
    border: none
.title
    text-shadow: -2px -2px rgb(200,200,200)
    font-size: 20px
    font-family: thimes
    color: black
    font-style: italic
.ui-footer
    text-align: right
    background: transparent
    border-color: transparent
    .ui-btn, .ui-btn *
        //margin-right: 20px
        //.fcLink, .fcLink *
        background-size: 100% 100%
        background-position: center center
        background: transparent
        border: none !important
        box-shadow: none !important
        webkit-box-shadow: none !important
    .ui-btn:last-child
        margin-right: 25px
.footerIcon
    height: 45px
    width: 45px

.popup
    text-align: center
    .popupContent
        padding: 15px
    .infoMain
        font-size: 22px
        text-align: center
        margin-bottom: -12px
        .left
            float: left
        .contactLogo
            margin-bottom: 5px
    .info
        font-size: 17px
    .name
        font-size: 18px
        font-style: italic
    input
        text-align: center

    .facebook
        line-height: 60px
    .logo
        font-size: 20px
        color: rgb(64,64,250)
        color: inherit
    .fcLogo
        height: 40px
        width: 40px
    img
        height: 90px // !important
        width: auto //!important
a.noUnderline
    text-decoration: none

